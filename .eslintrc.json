{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "linebreak-style": "off",
        "quotes": "off",
        "import/extensions": {
            "js": "never"
        }
    }
}